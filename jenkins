pipeline {
    agent {
         node {
            label 'dotnet'
            }
    }
    stages {
        
        stage('vcs') {

            steps {
                git url: 'https://github.com/Praveen-711/dotnetcore-docs-hello-world.git' 
                    branch: 'master'
            }
        }
        stage ('build'){
             steps{

                   dotnet build
                   
                
        }
    }
    
        stage ('publish') {
            steps {
                
                dotnet publish
                   
            }
    } 
    }
}
    