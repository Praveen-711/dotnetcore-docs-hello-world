pipeline {
    agent {
         node {
            label 'dotnet'
            }
    }
    stages {
        
        stage('vcs') {

            steps {
                git url: 'https://github.com/Praveen-711/dotnetcore-docs-hello-world.git' ,
                    branch: 'master'
            }
        }
        stage ('build'){
             steps{

                 sh "dotnet build"
                 sh "dotnet publish"
                   
                
       
    }
}
   stage('artifacts'){
    steps{
        archive '/home/ubuntu/remote_root/workspace/dot/bin/Debug/net6.0/publish/'
    }
   }
    }
}